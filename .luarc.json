{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "format.defaultConfig": {
    "indent_style": "space",
    "indent_size": "2",
    "quote_style": "single",
    "call_arg_parentheses": "keep",
    "detect_end_of_line": "true",
    "max_line_width": "80",
    "end_of_line": "lf"
  },
    "completion.autoRequire": false
}
